<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.CrowdWiseBali" parent="Theme.Material3.DayNight">
        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/blue_500</item>
        <item name="colorPrimaryVariant">@color/blue_700</item>
        <item name="colorOnPrimary">@color/white</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/sky_blue</item>
        <item name="colorSecondaryVariant">@color/sky_blue_300</item>
        <item name="colorOnSecondary">@color/black</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor">?attr/colorPrimaryVariant</item>
        <!-- Customize your theme here. -->
    </style>

    <style name="Theme.CrowdWiseBali.No.Bar" parent="Theme.AppCompat.Light.NoActionBar">
        <!-- Tambahkan atribut lainnya jika diperlukan -->
        <item name="android:windowIsTranslucent">true</item>
    </style>

</resources>